{
  "name": "atomictemplate",
  "version": "1.1.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "eslint && tsc --noEmit && exec next build --turbopack",
    "dev": "exec next dev --turbopack",
    "start": "next start",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "test:ci": "jest --runInBand",
    "test:watch": "jest --watch",
    "doctor": "pnpm typecheck && pnpm lint",
    "clean-cache": "rm -rf .next node_modules/.cache tsconfig.tsbuildinfo .eslintcache",
    "clean-local-git": "git branch | grep -v \"$(git rev-parse --abbrev-ref HEAD)\" | xargs -r git branch -D && git fetch --all --prune"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": "eslint --fix"
  },
  "description": "Production-ready Next.js template powered by atomic design methodology and data-driven architecture.",
  "template_owner": "nomanjawad",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomanjawad/atomictemplate.git"
  },
  "homepage": "https://github.com/nomanjawad/atomictemplate#readme",
  "bugs": {
    "url": "https://github.com/nomanjawad/atomictemplate/issues"
  },
  "copyright": "Â© 2025 nomanjawad. All rights reserved.",
  "dependencies": {
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "lucide-react": "^0.555.0",
    "next": "^15.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3"
  }
}
